{
  "manifest_version": 3,
  "name": "PulseTab",
  "version": "1.0.0",
  "description": "An elegant New Tab page for Chrome",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [],
  "host_permissions": ["https://wttr.in/*", "https://images.unsplash.com/*"]
}
